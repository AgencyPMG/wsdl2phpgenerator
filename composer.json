{
    "name": "pmg/wsdl2phpgenerator",
    "description": "Simple class library for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wsdl2PhpGenerator\\": ["src/", "lib/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wsdl2PhpGenerator\\Tests\\": ["tests/src"]
        }
    },
    "require": {
        "php": "~7.2",
        "ext-soap": "*",
        "symfony/options-resolver": "~3.0|~4.0",
        "symfony/polyfill-iconv": "~1.8"
    },
    "require-dev": {
        "php-vcr/php-vcr": "~1.4",
        "phpunit/phpunit": "~7.0",
        "psr/log": "~1.0"
    }
}
